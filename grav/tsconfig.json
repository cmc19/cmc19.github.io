{
    "version": "1.5.0-alpha",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "sourceMap": true
    },
    "filesGlob": [
        "./**/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "./main.ts",
        "./scripts/color.d.ts",
        "./typings/createjs-lib/createjs-lib.d.ts",
        "./typings/easeljs/easeljs.d.ts",
        "./typings/jquery/jquery.d.ts",
        "./typings/tweenjs/tweenjs.d.ts"
    ]
}
